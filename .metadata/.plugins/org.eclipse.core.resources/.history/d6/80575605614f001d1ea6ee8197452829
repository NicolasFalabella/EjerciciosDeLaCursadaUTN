#include <stdio.h>
#include <stdlib.h>

int contadorPositivosYNegativos(int contadorPositivos, int contadorNegativos) {

	int buffer;

	if (buffer > 0) {
		contadorPositivos++;
	} else {
		contadorNegativos++;
	}

 return buffer;
}
