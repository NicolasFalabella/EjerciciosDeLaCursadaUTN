#include <stdio.h>
#include <stdlib.h>

#define TAM_ARRAY 10

int listadoNumerosIngresados(int arrayNumero[]) {

	for (int i = 0; i < TAM_ARRAY; i++) {
		printf("numero %d %d \n", i + 1, arrayNumero[i]);
	}
	return arrayNumero;
}

int listadoDeLosNumerosPares(int arrayNumero[]){

	int contadorAuxiliar;

	for(int i; i < TAM_ARRAY; i++){
		if(arrayNumero[i] %2 == 0){
			contadorAuxiliar++;
		printf("numero %d %d \n", i+1, arrayNumero[i]);
		}
	}
	return 0;
}
